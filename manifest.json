         "js/jquery-1.7.2.min.js", 
         "js/md5.js", 
         "js/tVote.js", 
         "js/ratings.js", 
         "js/Settings/LocalStorageSettings.js", 
         "js/Settings/ChromeStorageSettings.js", 
         "js/Settings/Settings.js", 
         "js/myscript.js" ],
